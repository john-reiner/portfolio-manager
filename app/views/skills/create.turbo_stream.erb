<%= turbo_stream.prepend "skills" do %>
    <%= render  "skill", portfolio: @portfolio, skill_category: @skill_category, skill: @skill %>
<% end %>

<%= turbo_stream.replace "#{dom_id(Skill.new)}_form" do %>
    <%= render  "form", portfolio: @portfolio, skill_category:@skill_category, skill: Skill.new %>
<% end %>